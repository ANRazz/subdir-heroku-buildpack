#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir> <env-dir>

# Ensure wildcards in globs match dotfiles too.
shopt -s dotglob

BUILD_DIR=${1:-}
CACHE_DIR=${2:-}
ENV_DIR=${3:-}

ls $ENV_DIR

PATH_FILE_1="`cat $ENV_DIR/PROJECT_PATH`"
PATH_FILE_2="`cat $ENV_DIR\PROJECT_PATH`"


echo "path_file_1 $PATH_FILE_1"
echo "path_file_2 $PATH_FILE_2"

echo "failure"
exit 1


